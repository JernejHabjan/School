          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=asistentslivko.database.windows.net;Initial Catalog=SlivkoDatabase;Persist Security Info=True;User ID=jernejvid;Password=Leibniz1" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
